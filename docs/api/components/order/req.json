{
  "CreateOrder": {
    "type": "object",
    "properties": {
      "customerID": {
        "type": "integer",
        "example": 1
      },
      "product": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "productID": {
              "type": "string",
              "example": "toy"
            },
            "quantity": {
              "type": "integer",
              "example": 2
            }
          }
        }
      },
      "orderDate": {
        "type": "timestamp",
        "example": "2023-06-23T10:50:41.712Z"
      },
      "priority": {
        "type": "string",
        "example": "low"
      }
    }
  }
}
